#include <stdio.h>

int main() {
    int pages[20], n, f, frame[10], index = 0, faults = 0;

    printf("Enter number of pages: ");
    scanf("%d", &n);

    printf("Enter pages: ");
    for(int i=0;i<n;i++) scanf("%d",&pages[i]);

    printf("Enter frame size: ");
    scanf("%d", &f);

    for(int i=0;i<f;i++) frame[i] = -1;

    for(int i=0;i<n;i++){
        int found = 0;
        for(int j=0;j<f;j++)
            if(frame[j] == pages[i]) found = 1;

        if(!found){
            frame[index] = pages[i];
            index = (index + 1) % f;
            faults++;
        }

        printf("Frames: ");
        for(int k=0;k<f;k++) printf("%d ", frame[k]);
        printf("\n");
    }

    printf("Page Faults = %d\n", faults);
    return 0;
}
